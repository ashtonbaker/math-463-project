@article{hauert_effects_1997,
	title = {Effects of {Increasing} the {Number} of {Players} and {Memory} {Size} in the {Iterated} {Prisoner}'s {Dilemma}: {A} {Numerical} {Approach}},
	volume = {264},
	issn = {0962-8452},
	shorttitle = {Effects of {Increasing} the {Number} of {Players} and {Memory} {Size} in the {Iterated} {Prisoner}'s {Dilemma}},
	url = {http://www.jstor.org/stable/50539},
	abstract = {The Prisoner's Dilemma has become a paradigm for the evolution of altruistic behaviour. Here we present results of numerical simulations of the infinitely iterated stochastic simultaneous Prisoner's Dilemma considering players with longer memory, encounters of more than two players as well as different pay-off values. This provides us with a better foundation to compare theoretical results to experimental data. We show that the success of the strategy Pavlov, regardless of its simplicity, is far more general by having an outstanding role in the iterated N-player N-memory Prisoner's Dilemma. Besides, we study influences of increased memory sizes in the iterated two-player Prisoner's Dilemma, and present comparisons to results of experiments with first-year students.},
	number = {1381},
	urldate = {2015-12-07},
	journal = {Proceedings: Biological Sciences},
	author = {Hauert, Ch. and Schuster, H. G.},
	year = {1997},
	pages = {513--519},
	file = {Hauert and Schuster - 1997 - Effects of Increasing the Number of Players and Me.pdf:/home/ashton/.zotero/zotero/s013iha3.default/zotero/storage/KM225VNI/Hauert and Schuster - 1997 - Effects of Increasing the Number of Players and Me.pdf:application/pdf}
}

