@book{bicchieri_dynamics_1997,
	title = {The {Dynamics} of {Norms}},
	isbn = {978-0-521-56062-7},
	abstract = {In the social sciences norms are sometimes taken to play a key explanatory role. Yet norms differ from group to group, from society to society, and from species to species. How are norms formed and how do they change? This 'state-of-the-art' collection of essays presents some of the best contemporary research into the dynamic processes underlying the formation, maintenance, metamorphosis and dissolution of norms. The volume combines formal modelling with more traditional analysis, and considers biological and cultural evolution, individual learning, and rational deliberation. In filling a significant gap in the current literature this volume will be of particular interest to economists, political scientists and sociologists, in addition to philosophers of the social sciences.},
	language = {en},
	publisher = {Cambridge University Press},
	author = {Bicchieri, Cristina and Jeffrey, Richard and Skyrms, Brian},
	year = {1997},
	keywords = {Mathematics / History \& Philosophy, Philosophy / Political, Science / Philosophy \& Social Aspects},
	file = {Bicchieri et al. - 1997 - The Dynamics of Norms.pdf:/home/ashton/Desktop/Bicchieri et al. - 1997 - The Dynamics of Norms.pdf:application/pdf}
}

@article{axelrod_evolution_1981,
	title = {The evolution of cooperation},
	volume = {211},
	issn = {0036-8075, 1095-9203},
	url = {http://www.sciencemag.org/content/211/4489/1390},
	doi = {10.1126/science.7466396},
	abstract = {Cooperation in organisms, whether bacteria or primates, has been a difficulty for evolutionary theory since Darwin. On the assumption that interactions between pairs of individuals occur on a probabilistic basis, a model is developed based on the concept of an evolutionarily stable strategy in the context of the Prisoner's Dilemma game. Deductions from the model, and the results of a computer tournament show how cooperation based on reciprocity can get started in an asocial world, can thrive while interacting with a wide range of other strategies, and can resist invasion once fully established. Potential applications include specific aspects of territoriality, mating, and disease.},
	language = {en},
	number = {4489},
	urldate = {2015-12-15},
	journal = {Science},
	author = {Axelrod, R. and Hamilton, W. D.},
	month = mar,
	year = {1981},
	pmid = {7466396},
	pages = {1390--1396},
	file = {Axelrod and Hamilton - 1981 - The evolution of cooperation.pdf:/home/ashton/Desktop/Axelrod and Hamilton - 1981 - The evolution of cooperation.pdf:application/pdf;Snapshot:/home/ashton/.zotero/zotero/s013iha3.default/zotero/storage/P4CH76NV/1390.html:text/html}
}

